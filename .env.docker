# Docker Environment Variables
# Copy this file to .env.local before running docker-compose

# REQUIRED: Your Google Maps API Key
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=YOUR_API_KEY_HERE

# OPTIONAL: Pinned Location Coordinates (defaults to Marina Bay Sands, Singapore)
NEXT_PUBLIC_PIN_LAT=1.2834
NEXT_PUBLIC_PIN_LONG=103.8607
